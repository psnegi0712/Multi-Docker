{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "negi007/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "negi007/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client"]
        }
    ]

}